{
  "name": "hyperf-plus/helper",
  "description": "A helper component for hyperf.",
  "license": "MIT",
  "keywords": [
    "php",
    "hyperf",
    "helper"
  ],
  "support": {
  },
  "require": {
    "php": ">=7.3",
    "hyperf/command": "^2.1",
    "hyperf/di": "~2.1.0"
  },
  "require-dev": {
    "hyperf/devtool": "~2.1.0"
  },
  "autoload": {
    "psr-4": {
      "HPlus\\Helper\\": "src/"
    },
    "files": [
      "./src/Helper/helper.php"
    ]
  },
  "extra": {
    "hyperf": {
      "config": "HPlus\\Helper\\ConfigProvider"
    }
  },
  "bin": [
  ],
  "scripts": {
    "cs-fix": "php-cs-fixer fix $1",
    "test": "phpunit --colors=always"
  }
}
